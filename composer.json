		"typo3/cms-seo": "^13.3",
		"typo3/cms-setup": "^13.3",
		"typo3/cms-sys-note": "^13.4",
		"typo3/cms-t3editor": "^13.3",
		"typo3/cms-tstemplate": "^13.3",
		"typo3/cms-viewpage": "^13.3",
		"typo3/cms-webhooks": "^13.3"
